      <xsd:enumeration value="Voltage"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="DigitizerType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="8bit"/>
      <xsd:enumeration value="12bit"/>
      <xsd:enumeration value="14bit"/>
      <xsd:enumeration value="16bit"/>
      <xsd:enumeration value="32bit"/>
      <xsd:enumeration value="64bit"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CMOSSensorExposureMode">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Global shutter"/>
      <xsd:enumeration value="Rolling shutter"/>
      <xsd:enumeration value="Other"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- ********************************************** -->
  <!--    4DN-BINA OME Extesion - Advanced/Confocal   -->
  <!-- ********************************************** -->

  <!-- ================================== -->
  <!--            EXPERIMENTAL            -->
  <!-- ================================== -->

  <!-- Beam manipulation elements -->
  <xsd:element name="MicrobeamManipulation">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo/>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=Experimental</xsd:documentation>
      <xsd:documentation>Category=BeamManipulation</xsd:documentation>
      <xsd:documentation>Description=Defines a microbeam operation and the region of the image it was applied to. The LightSourceSettingsRef element is a reference to the LightSourceSettings that were used in this case, including a reference to the light source that was used for this micromanipulation experiment (e.g....................... a laser used for photobleaching).</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ExperimenterRef" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ROIRef" minOccurs="1" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:appinfo/>
            <xsd:documentation>Description=This empty element refers to the predefined four dimensional ROI (Region of Interest) that was used for this micromanipulation experiment.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="LightSourceSettings" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This holds a reference to the LightSource (i.e. the ID attribute) that was used in this case as well as the setting that were applied to in in this case.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="Description" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Description=A description for the Microbeam Manipulation. [plain-text multi-line string] </xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:whiteSpace value="preserve"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element minOccurs="0" name="AnnotationRef" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation>Description=This is a simple multi-line comment or annotation describing this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="Reference"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="ID" use="required" type="MicrobeamManipulationID">
        <xsd:annotation>
          <xsd:documentation>Description=A Unique Identifier for this MicrobeamManipulation hardware component.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Type" use="required" type="MicrobeamManipulationType">
        <xsd:annotation>
          <xsd:documentation>Description=This field defines the type of micromanipulation performed.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- ================================== -->
  <!--              HARDWARE              -->
  <!-- ================================== -->

  <!-- Modality elements -->
  <xsd:element name="TIRFHardwareModule">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=SpecialModality</xsd:documentation>
      <xsd:documentation>Description=Total Internal Reflection Fluorescence (TIRF) microscopy is a modality in which fluorescence is excited in a thin layer by an evanescent wave produced by total internal reflection. This element describes a Hardware Module that is used to achieve TIRF microscopy.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=TIRFSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ManufacturerSpec">
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PrismRef"/>
            <xsd:element ref="AnnotationRef" maxOccurs="unbounded" minOccurs="0"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Method" type="TIRFMethod" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the approach used by this TIRF Hardware Module to produce TIRF illumination. In Objective-based TIRF, the excitation lightpath and the emission channel share the same optics, which provides high signals but might result in high background. In Prism-based TIRF excitation light enters the dovetail prism, reflects at the glass/water interface and escapes through the opposite facet without interferring with the emission channel . In Lightguide-based TIRF, excitation light enters the lightguide, reflects multiple times from its top and bottom surfaces to generate wide-area evanescent waves and escapes from the opposite side of the lightguide without interferring with the emission channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RIng" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures whether this TIRF Hardware Module employs Ring (also known as spinning-spot) TIRF (True) or Spot TIRF (False).</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Motorized" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures whether this TIRF Hardware Module employs a Motorized device to set the TIRF Angle (True) or not (False).</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Confocal elements -->
  <xsd:element name="ConfocalScannerGroup" abstract="true" type="ConfocalScanner">
    <xsd:annotation>
      <xsd:documentation>Description=According to ISO-10934-2-2007, a confocal microscope is a microscope in which, ideally, a point in the object plane is illuminated by a diffraction-limited spot of light, and light emanating from this point is focused upon and detected from an area smaller than the central area of the diffraction disc. The confocal principle leads to improved axial resolution by suppression of light from out-of-focus planes. In order to utilize this modality, a Confocal Scanner is used to form an Image of an extended Specimen region by controlling the direction of an outgoing Laser beam, the position of the Specimen or the location of the illuminated and detected spots simultaneously. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ConfocalScanner">
    <xsd:annotation>
      <xsd:documentation>Description=According to ISO-10934-2-2007, a confocal microscope is a microscope in which, ideally, a point in the object plane is illuminated by a diffraction-limited spot of light, and light emanating from this point is focused upon and detected from an area smaller than the central area of the diffraction disc. The confocal principle leads to improved axial resolution by suppression of light from out-of-focus planes. In order to utilize this modality, a Confocal Scanner is used to form an Image of an extended Specimen region by controlling the direction of an outgoing Laser beam, the position of the Specimen or the location of the illuminated and detected spots simultaneously. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="AnnotationRef"/>
    </xsd:sequence>
    <xsd:attribute name="ID" type="ConfocalScannerID" use="required">
      <xsd:annotation>
        <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
        <xsd:documentation>Tier=1</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Name" type="Denomination">
      <xsd:annotation>
        <xsd:documentation>Description=A User-defined Name for this component.</xsd:documentation>
        <xsd:documentation>Tier=1</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="NrOfScanPoints" type="xsd:int" use="required">
      <xsd:annotation>
        <xsd:documentation>Description=This field records the number of scanning points available for this scan head.</xsd:documentation>
        <xsd:documentation>Tier=2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="MaxFrameRate" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Description=This field captures the Maximum full Frame Rate available for this device, as specificed by the Manufacturer.</xsd:documentation>
        <xsd:documentation>Tier=2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="MaxFrameRateUnit" type="UnitsFrequency">
      <xsd:annotation>
        <xsd:documentation>Tier=2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="PointsConfiguration" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Description=This field records a description of the arrangement of scan points (can be spiral, linear, grid, other)</xsd:documentation>
        <xsd:documentation>Tier=3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="RasterScanner" substitutionGroup="ConfocalScannerGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Confocal</xsd:documentation>
      <xsd:documentation>Description=A Raster Scanner is typically used for systematic periodic scanning (raster scanning) of the Laser beam or of the Specimen position, either in one or in two dimensions. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=ConfocalScannerSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ConfocalScanner">
          <xsd:attribute name="Type" type="RasterScannerType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the type of this Raster Scanner. In a Galvanometer-mirror based system, two galvanometer scan mirrors (one for each axis) direct the laser beam to any XY position in the field of view. In a Resonant system laser positioning in the x-axis is provided by a special resonant scan mirror which oscillates at a fixed frequency.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="HorizontalScanningSpeed" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the maximal Horizontal Scanning Speed of this device.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="HorizontalScanningSpeedUnit" type="UnitsVelocity">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="VerticalScanningSpeed" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the maximal Vertical Scanning Speed of this device.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="VerticalScanningSpeedUnit" type="UnitsVelocity">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SpinningDisk" substitutionGroup="ConfocalScannerGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Confocal</xsd:documentation>
      <xsd:documentation>Description=A Spinning disk confocal scanner is a device in which the scanning of the illuminated and detected spots is performed simultaneously using an opaque disk called a Nipkow disk, which has many ideally identical small holes arranged in Archimedean spirals. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=ConfocalScannerSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ConfocalScanner">
          <xsd:attribute name="SingleDisk" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=This records whether this Spinning Disk Confocal Scanner uses Single-Disk (True) or Dual-Disk (False) technology.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DiskSpeed" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the maximal Disk rotation Speed of this device, according to the Manufactuer.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="DiskSpeedUnit" type="UnitsVelocity">
            <xsd:annotation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Acousto-Optical elements -->
  <xsd:element name="AcoustoOpticalDeviceGroup" abstract="true" type="AcoustoOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Description=This is a device that performs spatial, temporal, and spectral modulations of light on the basis of acousto-optic (AO) diffraction. AO diffraction is a phenomenon whereby an acoustic wave propagating in an optically transparent medium, produces a periodic modulation of the index of refraction of the medium via the elasto-optical effect and might therefore diffract portions of an incident light into one or more directions.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AcoustoOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Description=This is a device that performs spatial, temporal, and spectral modulations of light on the basis of acousto-optic (AO) diffraction. AO diffraction is a phenomenon whereby an acoustic wave propagating in an optically transparent medium, produces a periodic modulation of the index of refraction of the medium via the elasto-optical effect and might therefore diffract portions of an incident light into one or more directions.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="AcoustoOpticalDeviceID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=A User-defined Name for this component.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ContrastRatio" type="PercentFraction">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the reports on the ratio of maximum versus minimum transmittance.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Coating" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Description=An optical Coating consists of one or more thin layers of material deposited on an optical component such as a Lens or Mirror, which alters the way in which the optic reflects and transmits light. This field records the thin-film material used to coat the non linear crystal and reduce surface reflection. </xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationMethod" type="AttenuationMethodType">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the mean by which this optical element achieves out-of-band transmission attenuation.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationCoefficient" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the Attenuation Coefficient (AC) of this optical element. The AC (aka blocking coefficient) is a material property that characterizes the degree to which light is being blocked (i.e., scattered or absorbed) by it. AC is used as a measure of the out-of-band blocking of an optical Lens, Filter, Dichroic mirror over an extended range of the spectrum. AC is often reported in units of optical density (OD), whereas OD= -log10(T), and T=Transmittance (0≤T≤1).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningAccuracy" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field measures the accuracy with which the wavelength discriminating capacity of this device can be tuned or changed.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningAccuracyUnit" type="UnitsLength" use="required">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Polarization" type="PolarizationMode" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the polarization effect of this optical element. Dichroic beamsplitters (and, in fact, any thin-film interference coating that is used at non-normal angles-of-incidence) will cause some amount of polarization, the precise effect varying greatly with wavelength and with the particular coating design.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Bragg" type="xsd:boolean" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This indicates whether this device is operated under Bragg regime (True) or not. In Bragg regime, at one particular incidence angle , only one diffraction order is produced - the others are annihilated by destructive interference.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningResolution" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field measures the wavelength discriminating capacity of this device and it is defined as the full width at half-maximum (FWHM) of the main lobe from the selected optical output, and is a function of both wavelength and device configuration.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningResolutionUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningRate" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the time it takes for this device to be tuned without degrading resolution. This time is determined by the velocity of the acoustic wage as it traverses the aperture of this device.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningRateUnit" type="UnitsTime">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngleOfIncidence" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the optimal angle of incidence (AOI) for this mirror. The AOI is the angle between the optical axis of the incident light and the axis normal to the surface of the mirror. In specular reflection AOI is by definition equal to the Angle Of Reflection (AOR).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngleOfIncidenceUnit" type="UnitsAngle">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngularAperture" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the angular aperture is the half-angle of the cone of light entering the this device. It is, therefore, a measure of how much of the incident light actually passes through the device.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngularApertureUnit" type="UnitsAngle">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Material" type="AODMaterial">
          <xsd:annotation>
            <xsd:documentation>Description=This field describes the transparent crystal or glass material used to build this device, through which the light propagates and whose refractive properties can be modulated by acoustic waves of different frequencies.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Design" type="AODDesign">
          <xsd:annotation>
            <xsd:documentation>Description=This field describes wheher the acoustic wave is absorbed or reflected by the distal surface of the crystal.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Geometry" type="AODGeometry">
          <xsd:annotation>
            <xsd:documentation>Description=This field describes the the direction of the applied electric field with respect to the light beam. Longitudinal devices have the electric field in the direction of the light beam, which passes through holes in the electrodes. Transverse devices have the electric field perpendicular to the light beam.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OperatingTemperature" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the ambient temperature at which this device is designed to operate, according to the Manufacturer.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OperatingTemperatureUnit" default="°C" type="UnitsTemperature">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AcoustoOpticalBeamSplitter" substitutionGroup="AcoustoOpticalDeviceGroup" type="AcoustoOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Advanced</xsd:documentation>
      <xsd:documentation>Description=An AOBS is actually an AOTF used in an imaginative manner by Leica to replace the dichroic mirror usually used to separate the illumination and the detection paths in a confocal fluorescence Microscope. In practice the AOBS is programmed so that it leaves most of the incoming beam of light undeflected, and it only deflects light originating from specific laser lines. The AOBS allows the simultaneous deflection of multiple laser lines simultaneously towards the sample, while at the same time controlling the intensity of each line independently.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=AcoustoOpticalBeamSplitterSettings</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcoustoOpticalDeflector" substitutionGroup="AcoustoOpticalDeviceGroup" type="AcoustoOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Advanced</xsd:documentation>
      <xsd:documentation>Description=Acousto-optic deflectors are devices which can be used to deflect a laser beam in one direction by a variable angle, controlled by the frequency of an electrical signal. Essentially, such a device is an acousto-optic modulator which is operated with an electrical drive signal of constant power but a variable frequency. The direction of the diffracted beam is determined by a Bragg condition, which contains the sound wavelength and thus depends on the sound frequency. </xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=AcoustoOpticalDeflectorSettings</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcoustoOpticalModulator" substitutionGroup="AcoustoOpticalDeviceGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Advanced</xsd:documentation>
      <xsd:documentation>Description=An acousto-optic modulator (AOM) is an electronically-tunable device which can be used for controlling the direction and/or intensity of a laser beam by an acoustically-induced diffraction grating in a crystal. It is based on the acousto-optic effect, i.e. the modification of the refractive index of some crystal or glass material by the oscillating mechanical strain of a sound wave (photoelastic effect).</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=AcoustoOpticalModulatorSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="AcoustoOpticalDevice">
          <xsd:attribute name="Type" type="AOMType">
            <xsd:annotation>
              <xsd:documentation>Description=This field describes the category of this device.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AcoustoOpticalTuneableFilter" substitutionGroup="AcoustoOpticalDeviceGroup" type="AcoustoOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Advanced</xsd:documentation>
      <xsd:documentation>Description=This is kind tuneable filter, whose optical filtering properties can be tuned by virtue of acousto-optical deflection. As such, different wavelengths exits the crystal at different angles depending on the applied radio wave frequency. In an optical system in which the input light beam strikes the crystal at a specific angle and can only re-enter the system at a specific angle, then one can choose the wavelength of the light diffracted at the output angle angle by varying the frequency of the acoustic wave.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=AcoustoOpticalTuneableFilterSettings</xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <!-- Electro-Optical elements -->
  <xsd:element name="ElectroOpticalDeviceGroup" abstract="true" type="ElectroOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Description=This is is a device which can be used for controlling the power (→ intensity modulators), phase (→ phase modulators) or polarization of light with an electrical control signal. It typically contains one or two Pockels cells, and possibly additional optical elements such as polarizers.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ElectroOpticalDevice">
    <xsd:annotation>
      <xsd:documentation>Description=This is is a device which can be used for controlling the power (→ intensity modulators), phase (→ phase modulators) or polarization of light with an electrical control signal. It typically contains one or two Pockels cells, and possibly additional optical elements such as polarizers.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ManufacturerSpec">
        <xsd:sequence>
          <xsd:element ref="AnnotationRef" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="ElectroOpticalDeviceID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Name" type="Denomination" use="optional">
          <xsd:annotation>
            <xsd:documentation>Description=A User-defined Name for this component.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ModulationMode" type="ModulationModeList">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the describes the type of modulation this devices produces on a beam of light.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ContrastRatio" type="PercentFraction">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the reports on the ratio of maximum versus minimum transmittance.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Coating" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Description=An optical Coating consists of one or more thin layers of material deposited on an optical component such as a Lens or Mirror, which alters the way in which the optic reflects and transmits light. This field records the thin-film material used to coat the non linear crystal and reduce surface reflection.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationMethod" type="AttenuationMethodType">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the mean by which this optical element achieves out-of-band transmission attenuation.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttenuationCoefficient" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the Attenuation Coefficient (AC) of this optical element. The AC (aka blocking coefficient) is a material property that characterizes the degree to which light is being blocked (i.e., scattered or absorbed) by it. AC is used as a measure of the out-of-band blocking of an optical Lens, Filter, Dichroic mirror over an extended range of the spectrum. AC is often reported in units of optical density (OD), whereas OD= -log10(T), and T=Transmittance (0≤T≤1).</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningAccuracy" type="xsd:float" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This field measures the accuracy with which the wavelength discriminating capacity of this device can be tuned or changed.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningAccuracyUnit" type="UnitsLength" use="required">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningResolution" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field measures the wavelength discriminating capacity of this device and it is defined as the full width at half-maximum (FWHM) of the main lobe from the selected optical output, and is a function of both wavelength and device configuration.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningResolutionUnit" type="UnitsLength">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningRate" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the time it takes for this device to be tuned without degrading resolution. This time is determined by the velocity of the acoustic wage as it traverses the aperture of this device.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TuningRateUnit" type="UnitsTime">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngleOfIncidence" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the optimal angle of incidence (AOI) for this mirror. The AOI is the angle between the optical axis of the incident light and the axis normal to the surface of the mirror. In specular reflection AOI is by definition equal to the Angle Of Reflection (AOR).</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngleOfIncidenceUnit" type="UnitsAngle">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngularAperture" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the angular aperture is the half-angle of the cone of light entering the this device. It is, therefore, a measure of how much of the incident light actually passes through the device.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AngularApertureUnit" type="UnitsAngle">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OperatingTemperature" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the ambient temperature at which this device is designed to operate, according to the Manufacturer.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OperatingTemperatureUnit" default="°C" type="UnitsTemperature">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OperatingHumidity" type="PercentFraction">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the ambient humidity at which this device is designed to operate, according to the manufactuer.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WarmUpTime" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the time it takes for this device to reach operating conditions according to the Manufacturer.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WarmUpTimeUnit" type="UnitsTime">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="DeformableMirror" substitutionGroup="ElectroOpticalDeviceGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=LightModulation</xsd:documentation>
      <xsd:documentation>Description=A Deformable Mirror (DM) is a Mechano Electro-Optical Device that has a controllable spatially varying mirror surface to modulate the wavefront of a beam of light impinging on it. This type of device is typically used in adaptive optics.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=DeformableMirrorSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ElectroOpticalDevice">
          <xsd:attribute name="Type" type="DeformableMirrorType" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the type of this Deformable Mirror.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Actuators" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the number of Actuators available for this device.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ActuatorPitch" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the distance between the center of each Actuator in this device.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ActuatorPitchUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ActuatorStroke" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the maximum possible Actuator displacement available for this device.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ActuatorStrokeUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ResponseTime" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the rate at which this Deformable Mirror can change patterns.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ResponseTimeUnit">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ElectroOpticalModulator" substitutionGroup="ElectroOpticalDeviceGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=LightModulation</xsd:documentation>
      <xsd:documentation>Description=An Electro-Optical Modulator (EOM) is a device which can be used for controlling the power (→ intensity modulators), phase (→ phase modulators) or polarization of light with an electrical control signal.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=ElectroOpticalModulatorSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ElectroOpticalDevice">
          <xsd:sequence>
            <xsd:element ref="PockelsCell"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LiquidCrystalTuneableFilter" substitutionGroup="ElectroOpticalDeviceGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=LightModulation</xsd:documentation>
      <xsd:documentation>Description=This element describes a liquid crystal tunable filter (LCTF) is an optical filter that uses electronically controlled liquid crystal (LC) elements to transmit a selectable wavelength of light and exclude others.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=LiquidCrystalTuneableFilterSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ElectroOpticalDevice">
          <xsd:attribute name="SelectableBandwidth" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field indicates whether the bandwidth of this tuneable filter can be selected (True) or it is fixed (False).</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaximumPower" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field describes the maximum Power this device can tolerate (typically expressed in Joules).</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaximumPowerUnit" type="UnitsPower">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SpatialLightModulator" substitutionGroup="ElectroOpticalDeviceGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=LightModulation</xsd:documentation>
      <xsd:documentation>Description=A Spatial Light Modulator (SLM) is device that imposes a changeable spatially varying modulation on a beam of light. The modulation can be in Intensity or in Phase or both.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=SpatialLightModulatorSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ElectroOpticalDevice">
          <xsd:attribute name="Type" type="SLMType">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the category to which this Spatial Light Modulator belongs.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ArrayWidth" type="NonNegativeInt">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the number of spatial elements (pixels) this Spatial Light Modulator contains in the X direction.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ArrayHeight" type="NonNegativeInt">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the number of spatial elements (pixels) this Spatial Light Modulator contains in the Y direction.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ResponseTime" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the rate at which this Spatial Light Modulator can change patterns.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ResponseTimeUnit" type="UnitsTime">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Additional optics elements -->
  <xsd:element name="MicrolensArray">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Lens</xsd:documentation>
      <xsd:documentation>Description=Micro-lens arrays contain multiple micro-lenses forming a one- or two-dimensional array on a supporting substrate.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="AnnotationRef"/>
        <xsd:element ref="WavelengthRange" maxOccurs="unbounded" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="ID" type="ElectroOpticalDeviceID" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
          <xsd:documentation>Tier=1</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Name" type="Denomination" use="optional">
        <xsd:annotation>
          <xsd:documentation>Description=A User-defined Name for this component.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletDiameter" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the physical Dameter of the Lenslets that compose this Micro Lens Array.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletDiameterUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletPitch" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the distance between Lenslets in this Micro Lens Array.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletPitchUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletArrangement" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the arrangement of individual Lenslets on this Array.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletShape" type="LensletShape">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the geometrical shape (e.g., round vs. square) of the individual Lenslets composing this Micro Lens Array.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="LensletProfileShape" type="LensShapeType">
        <xsd:annotation>
          <xsd:documentation>Description=This field describes the shape of the profile (e.g., concave vs. convex) of the individual Lenslets that compose this Micro Lens Array.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Coating" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description=An optical Coating consists of one or more thin layers of material deposited on an optical component such as a Lens or Mirror, which alters the way in which the optic reflects and transmits light. This field records the thin-film material used to coat the non linear crystal and reduce surface reflection.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Reflectance" type="xsd:float" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the maximum amount of light this Micro Lens Array is capable of reflecting. It is represented as a fractional value from 0.0 to 1.0.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="SustrateMaterial" type="OpticalMaterialName">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the chemical composition or designation of the substrate of this Micro Lens Array. The substrate is typically composed of a glass material with low autofluorescence, such as UV-grade fused silica.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="FrontFocalLength" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the distance (f) measured along the optical axis from the front principal plane of this Micro Lens Array to the front focal plane (F).</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="BackFocalLength" type="xsd:float">
        <xsd:annotation>
          <xsd:documentation>Description=This field records the distance (f') measured along the optical axis from the rear principal plane of this Micro Lens Array to the rear focal plane (F').</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="OpticalDistanceUnit" type="UnitsLength">
        <xsd:annotation>
          <xsd:documentation>Tier=3</xsd:documentation>
          <xsd:documentation>Description=Use this unit for both Focal Length fields.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- Aperture elements -->
  <xsd:element name="DiffractionGrating" substitutionGroup="OpticalApertureGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Aperture</xsd:documentation>
      <xsd:documentation>Description=A Diffraction Grating is an optical component with a periodic structure that splits and diffracts light into several beams travelling in different directions.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=DiffractiveGratingSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalAperture">
          <xsd:attribute name="Period" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the spacing between the centers of two adjacent slits (d).</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PeriodUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="SpatialFrequency" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the number of slits (or lines) per unit length (1/d). Typically it is measured in terms of number of slits per millimiter.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Transmittive" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=This field records whether the periodic fine structure of this Diffraction Grating is Transmissive (True) and therefore refracts light, or is Reflective (False) and therefore reflects it.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ModulationMode" type="ModulationModeList">
            <xsd:annotation>
              <xsd:documentation>Description=This field indicates what aspect of the light beam is modulated by this device.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Ronchi" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=This field records whether this Diffraction Grating has a square wave profile, as defined by Ronchi (True) or a sine wave profile (False).</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MaskingPlate" substitutionGroup="OpticalApertureGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Aperture</xsd:documentation>
      <xsd:documentation>Description=This element describes an opaque Masking Plate positioned in the light path to block specific components of the light beam.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=MaskingPlateSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalAperture">
          <xsd:attribute name="Function" type="MaskingPlateFunction" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field describes the Function of this Spatial Mask.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Excitation" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records whether this Spatial Mask is placed in the Excitation (True) or the Emission (False) light path.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ApertureSize" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the maximum physical Width of individual Apertures present in this Spatial Mask.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ApertureSizeUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ApertureNr." type="xsd:int" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the Number of individual apertures present in this Spatial Mask.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ApertureShape" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the Shape of individual apertures present in this Spatial Mask.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Material" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the Material with which this Spatial Mask is constructed.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Pinhole" substitutionGroup="OpticalApertureGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Aperture</xsd:documentation>
      <xsd:documentation>Description=A pin-like aperture positioned axially to the Light Path, which can act as a spatial filter for a beam of light to clean up the output of lasers and / or to suppress out-of-focus light in a confocal Microscope. Specifically, in Confocal Microscopy, such Pinholes are diaphragm typically situated in a plane conjugate with the object, and they restrict the area in the object plane that is illuminated and/or from which light is collected.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
      <xsd:documentation>Model_Settings=PinholeSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalAperture">
          <xsd:attribute name="MaxDiameter" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the maximum physical diameter of this Pinhole.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MaxDiameterUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Shape" type="PinholeShapeList">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the Shape of this Pinhole.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Detector elements -->
  <xsd:element name="WaveFrontSensor" substitutionGroup="CameraGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=Detector.Camera</xsd:documentation>
      <xsd:documentation>Description=A two dimensional sensor for measuring the location of the wavefront, typically used in Adaptive Optics.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
      <xsd:documentation>Model_Settings=CameraSettings</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Camera">
          <xsd:attribute name="Type" type="WFSType" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the type of this Wavefront Sensor.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Accuracy" type="PercentFraction">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the specified Wavefront Accuracy of this sensor at Peak Wavelength (expressed as a fraction of the Wavelength). </xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Sensitivity" type="PercentFraction">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the specified Wavefront Sensitivity of this sensor at Peak Wavelength (expressed as a fraction of the Wvelength).</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <!-- Miscellaneous sub-elements -->
  <xsd:element name="PockelsCell">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=LightModulation</xsd:documentation>
      <xsd:documentation>Description=Pockels cells are the basic components of electro-optic modulators. A Pockels cell is a device consisting of an electro-optic crystal (with some electrodes attached to it) through which a light beam can propagate. The phase delay in the crystal (→ Pockels effect) can be modulated by applying a variable electric voltage. The Pockels cell thus acts as a voltage-controlled waveplate.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="HalfWaveVoltage" type="xsd:float" use="required">
        <xsd:annotation>
          <xsd:documentation>Description=This field record the Voltage required for inducing a phase change equivalent to a half an optical wavelength. In an amplitude modulator, the applied voltage has to be changed by this value in order to go from the operation point with minimum transmission to that with maximum transmission. The HalfWaveVoltage can be referred to as, Uπ, Uλ/2 or Vλ/2.</xsd:documentation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="HalfWaveVoltageUnit" type="UnitsElectricPotential" use="required">
        <xsd:annotation>
          <xsd:documentation>Tier=2</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="Material" type="PockelsCellMaterial">
        <xsd:annotation>
          <xsd:documentation>Description=This field describes the nonlinear crystal materials used to contruct this Pockels cell.</xsd:documentation>
          <xsd:documentation>Tier=3</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>


  <!-- ================================== -->
  <!--         ACQUISITION SETTINGS       -->
  <!-- ================================== -->

  <!-- Ligth path elements -->
  <xsd:element name="TIRFLightPath" type="LightPath">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=MicroscopeHardwareSpecifications</xsd:documentation>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=A description of the Ligth Path utilized for this TIRF experiment.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <!-- Settings -->
  <xsd:element name="TIRFSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=Total Internal Reflection Fluorescence (TIRF) microscopy is a modality in which fluorescence is excited in a thin layer by an evanescent wave produced by total internal reflection. This element defines the TIRF conditions that were used for this Experiment.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="TIRFHardwareModuleRef"/>
            <xsd:element ref="TIRFLightPath" minOccurs="0"/>
            <xsd:element minOccurs="0" ref="Description"/>
          </xsd:sequence>
          <xsd:attribute name="ID" type="LSID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=A Unique Identifier for this component.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="HILO" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Description=This field capture whether (True) or not (False) TIRF illumination for this Experiment was set up at or below the critical angle (63˚; true TIRF) or with oblique illumination (i.e., Highly Inclined and Laminated Optical - HILO).</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="TIRFAngle" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field captures the incidence Angle used in this case.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ConfocalScannerSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Confocal Scanner used during the Acquisiton of this Channel as well as a reference (the ID attribute) to the Confocal Scanner in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="Settings">
          <xsd:attribute name="ID " type="ConfocalScannerID" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This is the Unique Identifier of the device used in this case.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PresetPinholeSize" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=In case this Confocal Scanner (i.e., Spinning disk, Swept-field, or other Multi-point scanners) has several fixed Pinhole Sizes to choose from, this field records the Pinhole Size that was set during the Acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PresetPinholeSizeUnit" type="UnitsLength" use="optional">
            <xsd:annotation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PinholeSpacing" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=If applicable, this field records the Spacing between individual Pinholes that was set during the Acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="PinholeSpacingUnit" type="UnitsLength">
            <xsd:annotation>
              <xsd:documentation>Description=If applicable, this field records the Spacing between individual Pinholes that was set during the Acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ScanDirectionality" type="ConfocalScanDirectionality" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the ScanDirectionality that was set for this device during Acquisition. In Unidirectional scan mode, data are only acquired while the laser beam moves in one direction. In Bidirectional scan mode, one line is scanned in one direction, and the next line in the opposite direction. In other words, the return run, or flyback, of the laser beam is used for recording data as well, thus increasing the scanning frequency when using bidirectional scan mode. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods. </xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="IntegrationNumber" type="PositiveInt" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=Signal integration is generally utilized to improve the overall signal-to-noise ratio of the resulting Image. This field records the number of sequential Scanning Units (Line, Frame or Stack) that have been integrated for the Acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="IntegrationUnit" type="ScanIntegrationMode" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=Signal integration is generally utilized to improve the overall signal-to-noise ratio of the resulting Image. This field refers to the nature of the Scanning Units used as the basis of signal integration. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="IntegrationMethod" type="ScanIntegrationMethod" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=Signal integration is generally utilized to improve the overall signal-to-noise ratio of the resulting Image. This field defines the signal Integration Method utilized for the Acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="EffectiveZoom" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field describes the Effective Zoom factor that was utilized for the acquisition of this Image (e.g., zoom = 1.3). In confocal microscopy, it is possible to set a given hardware-based Zoom factor, aka "Confocal Zoom" or "Scan Zoom", by reducing the angle at which the scanning mirrors operate. As a result, the scanned area will be reduced in size, which (because the pixel areas associated with the scan are kept constant) will decrease the effective size of the portion of the sample that is projected on each pixel thus increasing the effective resolving power of the system. For example, with a 512x512 pixel area, and with a 40x objective, zoom 1 we result in a pixel size of 732nm, while zoom 10 will reduce the pixel size to 73nm).In confocal microscopy, it is possible to set a given hardware-based Zoom factor, aka "Confocal Zoom" or "Scan Zoom", by reducing the angle at which the scanning mirrors operate. As a result, the scanned area will be reduced in size, which (because the pixel areas associated with the scan are kept constant) will decrease the effective size of the portion of the sample that is projected on each pixel thus increasing the effective resolving power of the system. For example, with a 512x512 pixel area, and with a 40x objective, zoom 1 we result in a pixel size of 732nm, while zoom 10 will reduce the pixel size to 73nm).</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ZoomAreaWidth" type="PositiveInt" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the Width of the Zoom Area that was scanned during the Acquisition of this Channel, expressed in Pixels.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ZoomAreaHeight" type="PositiveInt" use="optional">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the Height of the Zoom Area that was scanned during the Acquisition of this Channel, expressed in Pixels.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Descanned" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field indicates whether or not the Acquisition of this Image involved descanning.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ScanFormat" type="ConfocalScanFormat" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field defines the format of the scanned element utilized for this Acquisition. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="MultiChannelMode" type="ConfocalMultiChannelMode" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This field defines whether the the the Aquisiton of Multiple Channels for this Image happened in a Parallel (i.e., multiple channels acquired at the same time), or Sequentail (i.e., one channel after the other) manner.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ScanTime" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This records the time in seconds required to acquire a given number of lines, frames or stacks including the time required for repositioning the scanning device to the starting point. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ScanTimeUnit" type="UnitsTime" use="required">
            <xsd:annotation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name=" ConfocalFieldNumber" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field recors the Field Number of the Confocal scan optic is expressed as the objective magnification times the maximum diameter of the scanned field; [mm], obtained without shifting the position of the specimen and without stitching single images into a total larger image. Because images generally have a square format, the diameter of the scanned field is equals to the square root of 2 times the length of X dimension of the scanned field. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Rotation" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the Rotation angle of the scan region.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="RotationUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ScanningFrequency" type="PercentFraction">
            <xsd:annotation>
              <xsd:documentation>Description=This field records the Scanning Frequency is defined at the ratio between the Number of scanning element (Lines, Frames or Stacks) acquired and ScanTime. Refer to https://www.iso.org/standard/69820.html for further details and measurement methods.</xsd:documentation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AcoustoOpticalDeviceSettingsGroup" abstract="true" type="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element refers to the Acousto Optical Device used to acquire this image and it holds the settings that were applied to it during this Acquisition.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element refers to the Acousto Optical Device used to acquire this image and it holds the settings that were applied to it during this Acquisition.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Settings">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="TransmittanceProfileFile"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ReflectanceProfileFile"/>
          <xsd:element maxOccurs="unbounded" ref="TransmissionWavelengthRangeSettings" minOccurs="0"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ReflectionWavelengthRangeSettings"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="AcoustoOpticalDeviceID" use="required">
          <xsd:annotation>
            <xsd:documentation>Description=This is the Unique Identifier of the device used in this case.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DiffractionEfficiency" type="PercentFraction">
          <xsd:annotation>
            <xsd:documentation>Description=This field records a measure of the performance efficiency of this device. It's a measure of how much optical power is diffracted into a designated direction compared to the power incident onto the diffractive element.</xsd:documentation>
            <xsd:documentation>Tier=2</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AcousticVelocity" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the acoustic wave transit time across the crystal used during this Acquisition.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AcousticVelocityUnit" type="UnitsVelocity">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AcousticFrequency" type="xsd:float">
          <xsd:annotation>
            <xsd:documentation>Description=This field records the frequency of the acoustic wave used to alter the diffraction properties of the crystal during this Acquisition. Wavelength tuning is very rapid and limited only by the acoustic transit time across the crystal.</xsd:documentation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AcousticFrequencyUnit" type="UnitsFrequency">
          <xsd:annotation>
            <xsd:documentation>Tier=3</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AcoustoOpticalTuneableFilterSettings" substitutionGroup="AcoustoOpticalDeviceSettingsGroup" type="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to an Acousto Optical Tunable Filter (AOTF) used during the Acquisition of this Channel as well as a reference (the ID attribute) to the AOTF in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcoustoOpticalBeamSplitterSettings" substitutionGroup="AcoustoOpticalDeviceSettingsGroup" type="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to an Acousto Optical Beam Splitter (AOBS) used during the Acquisition of this Channel as well as a reference (the ID attribute) to the AOBS in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcoustoOpticalDeflectorSettings" substitutionGroup="AcoustoOpticalDeviceSettingsGroup" type="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to an Acousto Optical Deflector used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Acousto Optical Deflector in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AcoustoOpticalModulatorSettings" substitutionGroup="AcoustoOpticalDeviceSettingsGroup" type="AcoustoOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to an Acousto Optical Modulator used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Acousto Optical Modulator in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ElectroOpticalDeviceSettingsGroup" abstract="true" type="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element holds the Setting applied to an Electro-Optical Device used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Electro-Optical Device in question.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Description=This element refers to the Electro-Optical Device used to acquire this image and it holds the settings that were applied to it during this Acquisition.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="Settings">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="TransmittanceProfileFile"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ReflectanceProfileFile"/>
          <xsd:element maxOccurs="unbounded" ref="TransmissionWavelengthRangeSettings" minOccurs="0"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ReflectionWavelengthRangeSettings"/>
        </xsd:sequence>
        <xsd:attribute name="ID" type="ElectroOpticalDeviceID">
          <xsd:annotation>
            <xsd:documentation>Description=This is the Unique Identifier of the device used in this case.</xsd:documentation>
            <xsd:documentation>Tier=1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="DeformableMirrorSettings" substitutionGroup="ElectroOpticalDeviceSettingsGroup" type="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Deformable Mirror used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Deformable Mirror in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ElectroOpticalModulatorSettings" substitutionGroup="ElectroOpticalDeviceSettingsGroup" type="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to an Electro-Optical Modulator used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Electro-Optical Modulator in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PockelCellSettings" substitutionGroup="ElectroOpticalDeviceSettingsGroup" type="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a PockelCell used  during the Acquisiton of this Channel as well as a reference (the ID attribute) to the PockelCell in question.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LiquidCrystalTuneableFilterSettings" substitutionGroup="ElectroOpticalDeviceSettingsGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Liquid Crystal Tunable Filter (LCTF) used during the Acquisition of this Channel as well as a reference (the ID attribute) to the LCTF in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ElectroOpticalDeviceSettings">
          <xsd:attribute name="PolarizationRotation" type="xsd:float">
            <xsd:annotation>
              <xsd:documentation>Description=A Pockels cell is a device consisting of an electro-optic crystal (with some electrodes attached to it) through which a light beam can propagate. The phase delay in the crystal (→ Pockels effect) can be modulated by applying a variable electric voltage. Alignment of the crystal axis with the light beam axis is critical. This field records the polarization rotation applied to the beam during the acquisition of this Channel.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SpatialLightModulatorSettings" substitutionGroup="ElectroOpticalDeviceSettingsGroup" type="ElectroOpticalDeviceSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Spatial Light Modulator used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Spatial Light Modulator in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DiffractionGratingSettings" substitutionGroup="OpticalApertureSettingsGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Diffraction Grating used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Diffraction Grating in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalApertureSettings">
          <xsd:attribute name="DiffractionEfficiency" type="PercentFraction">
            <xsd:annotation>
              <xsd:documentation>Description=This is a measure of the performance efficiency of this device. It's a measure of how much optical power is diffracted into a designated direction compared to the power incident onto the diffractive element.</xsd:documentation>
              <xsd:documentation>Tier=2</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MaskingPlateSettings" substitutionGroup="OpticalApertureSettingsGroup" type="OpticalApertureSettings">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Masking Plate used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Masking Plate in question.</xsd:documentation>
      <xsd:documentation>Tier=2</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PinholeSettings" substitutionGroup="OpticalApertureSettingsGroup">
    <xsd:annotation>
      <xsd:documentation>Extension=Advanced+Confocal</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=Settings</xsd:documentation>
      <xsd:documentation>Description=This element holds the Setting applied to a Pinhole used during the Acquisition of this Channel as well as a reference (the ID attribute) to the Pinhole in question.</xsd:documentation>
      <xsd:documentation>Tier=1</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="OpticalApertureSettings">
          <xsd:attribute name="Aperture" type="xsd:float" use="required">
            <xsd:annotation>
              <xsd:documentation>Description=This refers to the Diameter of the Pin Hole's Aperture that was set for this Acquisition, expressed in Airy units.</xsd:documentation>
              <xsd:documentation>Tier=1</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>


