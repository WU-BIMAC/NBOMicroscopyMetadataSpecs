              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="ProjectionAngleUnit" type="UnitsAngle">
            <xsd:annotation>
              <xsd:documentation>Tier=3</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:extension>
