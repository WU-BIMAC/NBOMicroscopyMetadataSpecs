  <xsd:element name="TransmissionWavelengthRangeSettings">
    <!-- top level definition -->
    <xsd:annotation>
      <xsd:appinfo><xsdfu xmlns="">
	<plural>TransmissionWavelengthRangeSettings</plural>
</xsdfu>
      </xsd:appinfo>
      <xsd:documentation>Extension=Basic</xsd:documentation>
      <xsd:documentation>Domain=ImageAcquisitionSettings</xsd:documentation>
      <xsd:documentation>Category=ChildElement</xsd:documentation>
      <xsd:documentation>Description=This element records the range of Transmission Wavelengths that were set for this optical component during the Acquisition of this Image. It also records the maximum amount of light transmitted. Multiple of this elements can be used.</xsd:documentation>
